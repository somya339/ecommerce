<%if(success && success.length){%>
<div
    class="alert alert-success alert-dismissible fade show mt-2 d-flex justify-content-between"
    role="alert"
>
    <%=success%>
    <i class="fa fa-close btn" data-bs-dismiss="alert" aria-label="Close"></i>
</div>
<%}%> <%if(error && error.length){%>
<div
    class="alert alert-danger alert-dismissible fade show mt-2 d-flex justify-content-between"
    role="alert"
>
    <%=error%>
    <i class="fa fa-close btn" data-bs-dismiss="alert" aria-label="Close"></i>
</div>
<%}%>
